{
  "name": "frogment",
  "version": "1.0.2",
  "type": "module",
  "files": [ "dist" ],
  "repository": {
    "type": "git",
    "url": "https://github.com/haandev/frogment"
  },
  "keywords": [ "frogment", "template strings array", "literal", "sql", "query builder" ],
  "author": "haandev",
  "license": "MIT",
  "homepage": "https://github.com/haandev/frogment",
  "description": "Frogment a tool for build literal templates from fragments",
